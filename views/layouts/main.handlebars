                 <a class="nav-link"
                 href="/">
                <button
                  id="logoutBtn"
                  class="btn btn-outline-dark btn-rounded no-button shadow-pop-bottom-right"
                  data-mdb-ripple-color="dark"
                >Logout</button>
                </a>
              {{else }}
              <div class="d-flex align-items-center">
                <ul class="navbar-nav me-5 start-center">
                  <li class="nav-item">

                  <a class="nav-link"
                  href="/login">
