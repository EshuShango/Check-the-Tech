{{!-- {{#if logged_in}} --}}






<!-- POST -->
  {{#each posts as |post|}}
<div style="border: 2px solid red">

    <label>
      {{post.id}}
    </label>

    <h1>
      {{post.p_title}}

    </h1>
    <p>
      {{post.p_content}}

    </p>

</div>
  {{/each}}

  
